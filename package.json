{
  "name": "klever-desktop",
  "version": "2.0.4",
  "description": "GUI Wrapper for AppAgent - Electron-based desktop application that makes Python-based UI automation accessible to non-developers. Wraps the external AppAgent open-source project with a user-friendly interface for Android (ADB) and Web (Playwright) automation.",
  "main": ".vite/build/index.js",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc && tsc -p tsconfig.main.json",
    "preview": "vite preview",
    "electron": "electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "python:verify": "node scripts/verify-bundle.js",
    "python:sync": "node scripts/python-sync.js",
    "python:sync:all": "node scripts/python-refresh.js",
    "appagent:sync": "node scripts/appagent-sync.js"
  },
  "keywords": [
    "electron",
    "desktop",
    "ui-automation",
    "ollama",
    "ai"
  ],
  "author": "JooHyung Park",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FigmaAI/KleverDesktop.git"
  },
